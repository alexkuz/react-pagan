{
  "name": "react-pagan",
  "version": "0.0.5",
  "description": "Component for internationalization (enhanced version of React-Intl)",
  "scripts": {
    "build-lib": "./node_modules/.bin/babel src --out-dir lib",
    "build-demo": "NODE_ENV=production webpack -p",
    "stats": "webpack --profile --json > stats.json",
    "start": "node server.js",
    "lint": "eslint src",
    "version": "npm run build-demo && git add -A .",
    "postversion": "git push",
    "prepublish": "npm run build-lib"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkuz/react-pagan.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "intl",
    "i18n"
  ],
  "author": "Alexander <alexkuz@gmail.com> (http://kuzya.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexkuz/react-pagan/issues"
  },
  "homepage": "https://github.com/alexkuz/react-pagan",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.1.2",
    "babel-runtime": "^5.8.20",
    "cookie.js": "^2.1.1",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.2.3",
    "highlight.js": "^8.7.0",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.2",
    "promise-loader": "git+https://github.com/alexkuz/promise-loader.git#ad79ec1450da2c65fd2d423b623ee84e39be3ede",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "^0.24.2",
    "react-highlight": "^0.5.0",
    "react-hot-loader": "^1.2.8",
    "react-pure-render": "^1.0.2",
    "react-redux": "^1.0.1",
    "redux": "^1.0.1",
    "redux-pagan": "0.0.1",
    "redux-thunk": "^0.1.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "peerDependencies": {
    "react": ">=0.13.0 || ^0.14.0-alpha",
    "intl-format-cache": "^2.0.4",
    "intl-messageformat": "^1.1.0"
  }
}
